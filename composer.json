{
    "name": "rpr/swoft-sentry",
    "type": "library",
    "keywords": [
        "swoft2",
        "log",
        "sentry",
        "async"
    ],
    "description": "Sentry log component for Swoft2.0",
    "license": "Apache-2.0",
    "require": {
        "sentry/sentry": "1.10.0",
        "swoft/framework": "^2.0",
        "swlib/saber": "^1.0"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Gaodeng\\SwoftSentry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gaodeng\\SwoftSentry\\": "test/Cases"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
